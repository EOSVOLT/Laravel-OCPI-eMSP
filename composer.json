{
    "name": "eosvolt/laravel-ocpi-emsp",
    "description": "A Laravel package for OCPI (Open Charge Point Interface) protocol as eMSP (e-Mobility Service Provider).",
    "homepage": "https://github.com/EOSVOLT/Laravel-OCPI-eMSP",
    "keywords": [
        "laravel",
        "laravel-ocpi-emsp",
        "OCPI",
        "Open Charge Point Interface",
        "eMSP",
        "e-Mobility Service Provider"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "eosvolt",
            "email": "tech@eosvolt.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "^12.0",
        "illuminate/validation": "^12.0",
        "saloonphp/laravel-plugin": "^3.0",
        "saloonphp/saloon": "^3.0",
        "illuminate/database": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Ocpi\\": "src/",
            "Ocpi\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ocpi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ocpi\\OcpiServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
